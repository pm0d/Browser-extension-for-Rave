{
	"background": {
		"scripts": [ "js/background.js" ]
	},
	"browser_action": {
		"default_icon": "img/favicon.png",
		"default_title": "Rave"
	},
	"content_scripts": [
		{
			"css": [ "css/main.css" ],
			"js": [ "js/jquery-3.3.1.slim.min.js", "js/main.js" ],
			"matches": [ "https://beta.rave.office.net/*", "https://rave.office.net/*" ],
			"run_at": "document_idle"
		}
	],
	"description": "This extension is for injecting CSS and javascript in Rave",
	"icons": {
		"128": "img/favicon.png"
	},
	"manifest_version": 2,
	"name": "Rave",
	"permissions": [ "contextMenus", "tabs", "activeTab" ],
	"version": "1.0"
}
