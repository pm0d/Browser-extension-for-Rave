{
	"manifest_version": 2,
	"name": "Rave",
	"description": "This extension is for injecting CSS and javascript in Rave",
	"version": "1.0",
	"icons": {
		"128": "img/favicon.png"
	},
	"content_scripts": [
		{
			"matches": [ "https://beta.rave.office.net/*", "https://rave.office.net/*" ],
			"css": [ "css/main.css" ],
			"js": [ "js/jquery-3.3.1.slim.min.js", "js/main.js" ]
		}
	],
	"page_action": {
		"default_icon": "img/favicon.png",
		"default_popup": "popup.html"
	},
	"permissions": [ "activeTab" ]
}
