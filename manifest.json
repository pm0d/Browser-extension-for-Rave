{
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/favicon.png",
    "default_title": "DJ Rave"
  },
  "content_scripts": [
    {
      "css": ["css/content.css"],
      "js": ["js/jquery-3.3.1.slim.min.js", "js/content.js"],
      "matches": [
        "https://beta.rave.office.net/*",
        "https://rave.office.net/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Adds new logo, new theme, wide layout, context-menu search, helpful links, toggle panels, toggle proposed solution, fixes the Add button",
  "icons": {
    "128": "img/favicon.png"
  },
  "manifest_version": 2,
  "name": "DJ Rave",
  "permissions": ["contextMenus", "tabs", "activeTab"],
  "version": "3.2"
}
